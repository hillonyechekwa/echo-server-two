{
  "name": "echo-two-server",
  "version": "1.0.0",
  "description": "backend app for echo chat application",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "NODE_ENV=testing jest --forceExit --detectOpenHandles --silent",
    "test-routes": "npm test -t router",
    "test-models": "npm test -t model",
    "test-controllers": "npm test -t controllers",
    "test-auth": "npm test -t Authentication",
    "dev": "nodemon --exec npm run restart",
    "restart": "rimraf dist && npm run build && npm start",
    "start": "node dist/index.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:format": "prisma format",
    "studio": "prisma studio"
  },
  "author": "Hill Onyechekwa",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-object-rest-spread": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@prisma/client": "^5.12.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.3.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.12.1",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "graphql-scalars": "^1.23.0",
    "mongoose": "^8.3.2"
  }
}
